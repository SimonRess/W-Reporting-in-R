 volume = {v.318},
 publisher = {{Bertelsmann W. Verlag}},
 isbn = {978-3-7639-4005-9},
 series = {IAB-Bibliothek (Forschungsarbeiten)},
 editor = {Zika, Gerd and Schnur, Peter}
}


